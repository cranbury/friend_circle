<h1> Sign In <h1>

  <form action="<%= sesssion_url %>" method="post">
    <input
      name="authenticity_token"
      type="hidden"
      value="<%= form_authenticity_token %>">

    <label for="user_email">Email<label>
    <input
      id="user_email"
      name="user[email]"
      type="text">
    <br>

    <label for="user_password">Password<label>
      <input
      id="user-password"
      name="user[password]"
      type="password">
    <br>

    <input type="submit" value="Log in">
  <form>